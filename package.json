{
  "name": "ai-test-generator",
  "version": "0.1.7",
  "description": "AI-powered test generator for JavaScript and TypeScript codebases using Google Gemini",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-test-generator": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test-run": "ts-node test-run.ts"
  },
  "keywords": [
    "testing",
    "ai",
    "gemini",
    "google-ai",
    "test-generator",
    "typescript",
    "javascript",
    "jest",
    "mocha",
    "vitest"
  ],
  "author": "Kidus Yohannes",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kidyoh/ai-test-generator"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
